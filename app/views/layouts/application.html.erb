<!DOCTYPE html>
<html>
  <head>
    <title>GithubApi</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% if current_user %>
      <%= render partial: '/layouts/navbar' %>
    <% else %>
      <%= render partial: '/layouts/logged_out_nav' %>
    <% end %>
    <%= yield %>
  </body>
</html>
